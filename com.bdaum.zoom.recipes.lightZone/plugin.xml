<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.bdaum.zoom.ui.relationDetector">
      <detector
            class="com.bdaum.zoom.recipes.lightzone.internal.RelationDetector"
            description="%detector.description"
            id="lightzone"
            name="%detector.name">
      </detector>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.help.index">
      <index
            file="index.xml">
      </index>
   </extension>

</plugin>
