<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:catalog?after=catExt1">
         <menu
               icon="icons/import_wiz.gif"
               id="importForeignCat"
               label="%menu.label.import"
               mnemonic="%menu.mnemonic.import">
            <command
                  commandId="com.bdaum.zoom.cat.foreign.lightroom"
                  icon="icons/lr.png"
                  label="%command.label.lr"
                  mnemonic="%command.mnemonic.lr"
                  style="push"
                  tooltip="%command.tooltip.lr">
               <visibleWhen
                     checkEnabled="false">
                  <systemTest
                        property="com.bdaum.zoom.cat.access"
                        value="write">
                  </systemTest>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="com.bdaum.zoom.ui.CatalogCommands"
            defaultHandler="com.bdaum.zoom.cat.foreign.lr.internal.LightroomImportHandler"
            description="%command.description.lr"
            id="com.bdaum.zoom.cat.foreign.lightroom"
            name="%command.name.lr">
      </command>
   </extension>

</plugin>
